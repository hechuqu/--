					<image class="img" wx:if="{{item.image[8] != null}}" data-text='{{item.image[8]}}' bindtap="yulan"
						src="{{item.image[8]}}"></image>

				</view>

			<view class="t">
				<text>{{item.add}} {{item.createTime}} </text>

			</view>
			</view>

		</view>


	</van-tab>
</van-tabs>
