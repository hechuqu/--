					<view style="font-size: small;">

						{{item.name}}</view>
					<view class="ic">
						<v-book number="{{item.num}}" />

<view>
	<button data-text='{{item}}' open-type="share"> 分享 </button> </view>
						<image style="height:80rpx;width: 80rpx;" bindtap="tonghua" data-text="{{item.num}}" class=" icon1"
							src="https://dss2.bdstatic.com/70cFvnSh_Q1YnxGkpoWK1HF6hhy/it/u=3230099242,3567665174&fm=26&gp=0.jpg">

